<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.ljy.InvDAO">

 
    
     <select id="list_INV" resultType="com.test.ljy.InvDTO">
      	SELECT INVITE_ID, SENDER_ID, RIDING_ID, RECEIVER_ID, SEND_DATE
		FROM INVITE
    </select>
    
    <delete id= "delete" >
		DELETE
		FROM INVITE
		WHERE INVITE_ID = #{invite_id}
	</delete>
    
    
	
    
  

    
    
    </mapper>




